  <% include ../components/headed-text-linked.ejs %>
<% } %>
</div>
<aside>
  <div class="prepare-info">
  <p><strong>Get information on:</strong></p>
  <% if (page.includes && page.includes.length > 0) { %>
    <ul>
      <% page.includes.forEach(function(inc, i) { %>
