<% if (!req.disqualified) { %>
<div id="global-cookie-message" class="full">
  <div class="centralise">
    <p>We uses cookies to make the site simpler. <a href="/help/cookies">Find out more about cookies</a></p>
  </div>
</div>
<% } %>
<header role="banner" class="c-global-header">
  <div class="header-wrapper">
    <div class="header-global">
      <div class="header-logo">
        <a href="https://www.gov.uk" title="Go to the GOV.UK homepage" class="logo content" data-link-type="gds-logo">
          <img src="https://assets.publishing.service.gov.uk/static/gov.uk_logotype_crown_invert_trans-203e1db49d3eff430d7dc450ce723c1002542fe1d2bce661b6d8571f14c1043c.png" width="36" height="32" alt=""> GOV.UK
        </a>
      </div>
      <div class="header-proposition">
        <div class="content">
          <a href="/" id="proposition-name" data-link-type="service-name">Get help with child arrangements</a>
        </div>
      </div>
    </div>
  </div>
</header>
<% if (type != 'error') { %>
<div class="c-global-feedback c-global-header">
<div class="header-wrapper">
<h3>Tell us what you think</h3>
<p>Your <a href="https://www.surveymonkey.co.uk/r/3DXCX9L" data-link-type="feedback-banner">feedback</a> will help us make improvements</p>
</div>
</div>
<div class="c-global-header-bar"></div>
<% } %>