{
  "name": "oocp-cait",
  "version": "1.0.0",
  "engines": {
    "node": "4.4.5",
    "npm": "2.15.5"
  },
  "scripts": {
    "build": "gulp build",
    "githooks": "git config core.hooksPath .githooks",
    "githooksUnset": "git config --unset core.hooksPath",
    "lint": "yarn run lint:app && yarn run lint:client && yarn run lint:spec",
    "lintparallel": "sh ./parallel.sh",
    "lint:app": "eslint scripts lib",
    "lint:client": "eslint src/scripts",
    "lint:spec": "eslint spec",
    "postinstall": "yarn run build",
    "preinstall": "if [ -x \"$(command -v git)\" ]; then yarn run githooks; else echo 'git not installed'; fi;",
    "start": "node scripts/server.js",
    "test": "yarn run test:unit",
    "test:unit": "ava **/*.unit.spec.js"
  },
  "description": "Child Arrangements Informational Tool",
  "main": "scripts/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/oocp-cait.git"
  },
  "author": "Alex Ward",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ministryofjustice/oocp-cait/issues"
  },
  "homepage": "https://github.com/ministryofjustice/oocp-cait#readme",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.16.3",
    "basic-auth": "^1.1.0",
    "body-parser": "^1.15.2",
    "browser-sync": "^2.17.5",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "connect-browser-sync": "^2.0.1",
    "del": "latest",
    "ejs": "^2.5.2",
    "eslint": "latest",
    "express": "^4.14.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-casperjs": "0.0.6",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "latest",
    "gulp-express": "^0.3.5",
    "gulp-jasmine": "^2.4.2",
    "gulp-merge-media-queries": "^0.2.1",
    "gulp-plumber": "^1.1.0",
    "gulp-stylus": "^2.5.0",
    "jasmine-console-reporter": "^1.2.7",
    "jeet": "6.1.2",
    "jquery": "^2.2.0",
    "morgan": "^1.7.0",
    "node-notifier": "^4.6.1",
    "rupture": "^0.6.1",
    "serve-favicon": "^2.3.2"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-jsdoc": "^0.2.0",
    "eslint-plugin-jsdoc": "^2.4.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "webpack": "^1.14.0"
  }
}
