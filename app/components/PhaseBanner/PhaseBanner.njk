{% macro PhaseBanner() %}
{% if getString('ServicePhase') %}
<div class="PhaseBanner gv-c-phase-banner">
  <p class="gv-c-phase-banner__content">
    <strong class="gv-c-phase-tag">{{ Block.String('ServicePhase') }}</strong>
    <span class="gv-c-phase-banner__text">{{ Block.String('ServicePhase__text') | safe }}</span>
  </p>
</div>
{% endif %}
{% endmacro %}
